{
    "name": "webfactor_project/wf_basic_function_package",
    "description": "A package to add often needed, basic functions to a laravel project",
    "type": "library",
    "authors": [
        {
            "name": "Ben-Gentner",
            "email": "ben.gentner@webfactor.de"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "ziffmedia/nova-select-plus": "^1.0",
        "laravel/nova": "3.*",
        "ebess/advanced-nova-media-library": "^3.8"
    },
    "autoload": {

    },
    "extra": {
        "laravel": {
            "providers": [
                "Webfactor\\WfBasicFunctionPackage\\BasicFunctionsServiceProvider"
            ]
        }
